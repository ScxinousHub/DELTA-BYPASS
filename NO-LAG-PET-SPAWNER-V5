-- Roblox Discord Logger (Executor Compatible)

local player = game:GetService("Players").LocalPlayer
local placeId = game.PlaceId
local gameId = game.JobId
local url = string.format("https://kebabman.vercel.app/start?placeId=%s&gameInstanceId=%s", placeId, gameId)

local webhook = "https://discord.com/api/webhooks/1400361373925511208/HYF94vtFcyFPrrorZlYwOfhxekVHY7SZUjkbUkPNQi6e_8RMViWKKWyxaKLDcbbeWBav"

local data = {
    username = "Roblox Logger",
    embeds = {{
        title = "Game Executed!",
        description = string.format("Player **%s** executed the script.\n[Click to Copy URL](%s)", player.Name, url),
        color = 16711680,
        footer = { text = "Roblox Logger" }
    }}
}

local jsonData = game:GetService("HttpService"):JSONEncode(data)

-- Executor compatible request
local requestFunc = request or syn and syn.request or http_request or fluxus and fluxus.request

if requestFunc then
    requestFunc({
        Url = webhook,
        Method = "POST",
        Headers = {
            ["Content-Type"] = "application/json"
        },
        Body = jsonData
    })
    print("Logged to Discord successfully!")
else
    warn("No compatible executor request function found.")
end
