-- Executor-Compatible Discord Webhook Logger
local webhookUrl = "https://discord.com/api/webhooks/1400361373925511208/HYF94vtFcyFPrrorZlYwOfhxekVHY7SZUjkbUkPNQi6e_8RMViWKKWyxaKLDcbbeWBav"

local serverId = game.JobId
local playerName = game.Players.LocalPlayer.Name

local data = {
    content = "Server ID: " .. serverId, -- Message is just the server ID
    username = playerName
}

local jsonData = game:GetService("HttpService"):JSONEncode(data)

-- Executor detection
local success, response = pcall(function()
    if syn then
        -- Synapse X
        return syn.request({
            Url = webhookUrl,
            Method = "POST",
            Headers = {["Content-Type"] = "application/json"},
            Body = jsonData
        })
    elseif http then
        -- Krnl
        return http_request({
            Url = webhookUrl,
            Method = "POST",
            Headers = {["Content-Type"] = "application/json"},
            Body = jsonData
        })
    elseif request then
        -- Sentinel
        return request({
            Url = webhookUrl,
            Method = "POST",
            Headers = {["Content-Type"] = "application/json"},
            Body = jsonData
        })
    else
        error("Unsupported executor")
    end
end)

if success then
    print("✅ Webhook sent successfully with Server ID!")
else
    warn("❌ Failed to send webhook: "..tostring(response))
end
